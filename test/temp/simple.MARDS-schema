#!MARDS_schema_en_1.0 blah
    import sub
        local
        
define_type jed

name blink
    value
        type label
        required
    name rate
    insert abc
template abc
    value
        type jed
        required
    name joex
        required
        value
            type length
    name joey
    search joey
        match "zark"
            extend xx
                from sub
        match_else
            raise_error "We don't rightly support non-'joey zark' types around here"
name zed
    value
        type float
    name bob
    search bob
        match "55"
            extend rabbit
                from sub/subsub
name item
    treatment unique
    value
        type label
        required
    name size
        treatment one
        value
            type mass
        recurse size
            limit 1
    name color
        treatment unique
        value
            type radio_select
                choice "blue"
                    name blue_cave
                choice "red"
                    name red_cave
                choice "yellow"
                    name yellow_cave
        name intensity
            value
                type percent
    name title
        required
        treatment concat
        value
            default "unknown"